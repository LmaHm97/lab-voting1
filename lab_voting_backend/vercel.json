{
  "version": 2,
  "builds": [
    { "src": "src/main.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "handle": "filesystem" },

    { "src": "/assets/(.*)", "dest": "src/static/assets/$1" },
    { "src": "/favicon.ico", "dest": "src/static/favicon.ico" },

    { "src": "/(.*)", "dest": "src/main.py" }
  ]
}
